    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue": "^3.0.5",
    "vue-tsc": "^0.2.2"
  },
  "peerDependencies": {
    "vue": "^3"
